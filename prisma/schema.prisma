generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model bank_accounts_new {
  id           String  @id @db.VarChar(50)
  amount       Int?    @default(0)
  transactions String? @db.LongText
  auth         String? @db.LongText
  isFrozen     Int?    @default(0)
  creator      String? @db.VarChar(50)
  tl_businesses tl_businesses?
}

model tl_businesses {
  id              Int               @id @default(autoincrement())
  bank_account_id String            @unique @db.VarChar(50)
  label           String            @db.VarChar(255)
  category        String            @db.VarChar(100)
  map             String?           @db.VarChar(100)
  owner           String?           @db.VarChar(50)
  is_owned        Boolean           @default(false)
  created_at      DateTime          @default(now()) @db.Timestamp(0)
  updated_at      DateTime          @updatedAt @db.Timestamp(0)
  bank_account    bank_accounts_new @relation(fields: [bank_account_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "tl_businesses_bank_account_id_fkey")

  @@index([category], map: "tl_businesses_category_idx")
  @@index([map], map: "tl_businesses_map_idx")
  @@index([owner], map: "tl_businesses_owner_idx")
}

model leagues {
  id                   Int                    @id @default(autoincrement())
  name                 String                 @db.VarChar(255)
  status               String                 @db.VarChar(50)
  start_at             DateTime?              @db.Timestamp(0)
  end_at               DateTime?              @db.Timestamp(0)
  creator              String                 @db.VarChar(100)
  price                Int
  min_player           Int                    @default(0)
  max_team             Int                    @default(0)
  rules_json           Json?
  created_at           DateTime               @default(now()) @db.Timestamp(0)
  updated_at           DateTime               @updatedAt @db.Timestamp(0)
  league_teams         league_teams[]
  league_matches       league_matches[]

  @@index([status], map: "leagues_status_idx")
  @@index([creator], map: "leagues_creator_idx")
}

model league_teams {
  id                               Int                          @id @default(autoincrement())
  league_id                        Int
  code                             String                       @db.VarChar(100)
  name                             String                       @db.VarChar(255)
  status                           String                       @db.VarChar(50)
  joined_at                        DateTime?                    @db.Timestamp(0)
  created_at                       DateTime                     @default(now()) @db.Timestamp(0)
  updated_at                       DateTime                     @updatedAt @db.Timestamp(0)
  leagues                          leagues                      @relation(fields: [league_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "league_teams_league_id_fkey")
  home_league_matches              league_matches[]             @relation("league_matches_home_team_id_fkey")
  away_league_matches              league_matches[]             @relation("league_matches_away_team_id_fkey")
  league_match_roster_players      league_match_roster_players[]
  winning_league_match_results     league_match_results[]       @relation("league_match_results_winner_team_id_fkey")

  @@index([league_id], map: "league_teams_league_id_idx")
  @@index([code], map: "league_teams_code_idx")
  @@index([status], map: "league_teams_status_idx")
}

model league_matches {
  id                            Int                           @id @default(autoincrement())
  league_id                     Int
  home_team_id                  Int
  away_team_id                  Int
  scheduled_at                  DateTime?                     @db.Timestamp(0)
  round                         Int?
  stage                         String?                       @db.VarChar(50)
  zone                          String?                       @db.VarChar(100)
  status                        String                        @db.VarChar(50)
  created_at                    DateTime                      @default(now()) @db.Timestamp(0)
  updated_at                    DateTime                      @updatedAt @db.Timestamp(0)
  leagues                       leagues                       @relation(fields: [league_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "league_matches_league_id_fkey")
  home_team                     league_teams                  @relation("league_matches_home_team_id_fkey", fields: [home_team_id], references: [id], onDelete: Restrict, onUpdate: Restrict, map: "league_matches_home_team_id_fkey")
  away_team                     league_teams                  @relation("league_matches_away_team_id_fkey", fields: [away_team_id], references: [id], onDelete: Restrict, onUpdate: Restrict, map: "league_matches_away_team_id_fkey")
  league_match_roster_players   league_match_roster_players[]
  league_match_results          league_match_results?

  @@index([league_id], map: "league_matches_league_id_idx")
  @@index([home_team_id], map: "league_matches_home_team_id_idx")
  @@index([away_team_id], map: "league_matches_away_team_id_idx")
  @@index([status], map: "league_matches_status_idx")
  @@index([round], map: "league_matches_round_idx")
}

model league_match_roster_players {
  id              Int            @id @default(autoincrement())
  match_id        Int
  team_id         Int
  citizenid       String         @db.VarChar(50)
  created_at      DateTime       @default(now()) @db.Timestamp(0)
  updated_at      DateTime       @updatedAt @db.Timestamp(0)
  league_matches  league_matches @relation(fields: [match_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "league_match_roster_players_match_id_fkey")
  league_teams    league_teams   @relation(fields: [team_id], references: [id], onDelete: Restrict, onUpdate: Restrict, map: "league_match_roster_players_team_id_fkey")

  @@index([match_id], map: "league_match_roster_players_match_id_idx")
  @@index([team_id], map: "league_match_roster_players_team_id_idx")
  @@index([citizenid], map: "league_match_roster_players_citizenid_idx")
}

model league_match_results {
  id             Int            @id @default(autoincrement())
  match_id       Int            @unique(map: "league_match_results_match_id_key")
  home_score     Int            @default(0)
  away_score     Int            @default(0)
  result_status  String         @db.VarChar(50)
  winner_team_id Int?
  kill_logs_json Json?
  created_at     DateTime       @default(now()) @db.Timestamp(0)
  updated_at     DateTime       @updatedAt @db.Timestamp(0)
  league_matches league_matches @relation(fields: [match_id], references: [id], onDelete: Cascade, onUpdate: Restrict, map: "league_match_results_match_id_fkey")
  winner_team    league_teams?  @relation("league_match_results_winner_team_id_fkey", fields: [winner_team_id], references: [id], onDelete: SetNull, onUpdate: Restrict, map: "league_match_results_winner_team_id_fkey")

  @@index([winner_team_id], map: "league_match_results_winner_team_id_idx")
  @@index([result_status], map: "league_match_results_result_status_idx")
}

model payments {
  id                    Int              @id @default(autoincrement())
  invoice_number        String           @unique(map: "payments_invoice_number_key") @db.VarChar(128)
  provider              String           @db.VarChar(50)
  channel               String?          @db.VarChar(50)
  amount                Int
  currency              String           @default("IDR") @db.VarChar(10)
  status                String           @db.VarChar(50)
  checkout_url          String?          @db.VarChar(512)
  payer_account_id      Int?
  purpose_type          String           @db.VarChar(100)
  purpose_ref           String?          @db.VarChar(191)
  metadata_json         Json?
  provider_payload_json Json?
  paid_at               DateTime?        @db.Timestamp(0)
  expired_at            DateTime?        @db.Timestamp(0)
  created_at            DateTime         @default(now()) @db.Timestamp(0)
  updated_at            DateTime         @updatedAt @db.Timestamp(0)

  @@index([status], map: "payments_status_idx")
  @@index([purpose_type], map: "payments_purpose_type_idx")
  @@index([purpose_type, purpose_ref], map: "payments_purpose_lookup_idx")
  @@index([payer_account_id], map: "payments_payer_account_id_idx")
}

model bans {
  id       Int     @id @default(autoincrement())
  name     String? @db.VarChar(50)
  license  String? @db.VarChar(50)
  discord  String? @db.VarChar(50)
  ip       String? @db.VarChar(50)
  reason   String? @db.Text
  expire   Int?
  bannedby String  @default("LeBanhammer") @db.VarChar(255)

  @@index([discord], map: "discord")
  @@index([ip], map: "ip")
  @@index([license], map: "license")
}

model management_outfits {
  id         Int     @id @default(autoincrement())
  job_name   String  @db.VarChar(50)
  type       String  @db.VarChar(50)
  minrank    Int     @default(0)
  name       String  @default("Cool Outfit") @db.VarChar(50)
  gender     String  @default("male") @db.VarChar(50)
  model      String? @db.VarChar(50)
  props      String? @db.Text
  components String? @db.Text
}

model npwd_calls {
  id          Int     @id @default(autoincrement())
  identifier  String? @db.VarChar(48)
  transmitter String  @db.VarChar(255)
  receiver    String  @db.VarChar(255)
  is_accepted Int?    @default(0) @db.TinyInt
  isAnonymous Int     @default(0) @db.TinyInt
  start       String? @db.VarChar(255)
  end         String? @db.VarChar(255)

  @@index([identifier], map: "identifier")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model npwd_darkchat_channel_members {
  channel_id             Int
  user_identifier        String                 @db.VarChar(255)
  is_owner               Int                    @default(0) @db.TinyInt
  npwd_darkchat_channels npwd_darkchat_channels @relation(fields: [channel_id], references: [id], onUpdate: Restrict, map: "npwd_darkchat_channel_members_npwd_darkchat_channels_id_fk")

  @@index([channel_id], map: "npwd_darkchat_channel_members_npwd_darkchat_channels_id_fk")
  @@ignore
