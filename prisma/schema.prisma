}

model web_accounts {
  id             Int                     @id @default(autoincrement())
  email          String                  @unique @db.VarChar(255)
  password       String?                 @db.VarChar(255)
  email_verified Boolean                 @default(false)
  user_id        Int?                    @db.UnsignedInt
  discord_id     String?                 @unique @db.VarChar(30)
  fivem_id       String?                 @db.VarChar(20)
  created_at     DateTime                @default(now()) @db.Timestamp(0)
  updated_at     DateTime                @updatedAt @db.Timestamp(0)
  user           users?                  @relation(fields: [user_id], references: [userId])
  profile        web_profiles?
  discord        web_discord_accounts?
  sessions       web_sessions[]

  @@index([user_id])
  @@index([discord_id])
  @@index([fivem_id])
}

model web_discord_accounts {
  id           Int          @id @default(autoincrement())
  account_id   Int          @unique
  discord_id   String       @unique @db.VarChar(30)
  username     String       @db.VarChar(255)
  global_name  String?      @db.VarChar(255)
  email        String?      @db.VarChar(255)
  image        String?      @db.VarChar(255)

  account      web_accounts @relation(fields: [account_id], references: [id], onDelete: Cascade)

  created_at   DateTime     @default(now()) @db.Timestamp(0)
  updated_at   DateTime     @updatedAt @db.Timestamp(0)

  @@index([account_id])
  @@index([discord_id])
}

model web_profiles {
  id             Int          @id @default(autoincrement())
  account_id     Int          @unique
  real_name      String?      @db.VarChar(255)
  fivem_name     String?      @db.VarChar(255)
  gender         Gender?      
  birth_date     DateTime?    @db.Timestamp(0)
  province_id    String?      @db.VarChar(50)
  province_name  String?      @db.VarChar(100)
  city_id        String?      @db.VarChar(50)
  city_name      String?      @db.VarChar(100)
  created_at     DateTime     @default(now()) @db.Timestamp(0)
  updated_at     DateTime     @updatedAt @db.Timestamp(0)
  account        web_accounts @relation(fields: [account_id], references: [id], onDelete: Cascade)
}

enum Gender {
  male
  female
}

model web_sessions {
  id            Int          @id @default(autoincrement())
  session_token String       @unique @db.VarChar(255)
  user_id       Int
  expires       DateTime     @db.Timestamp(0)
  account       web_accounts @relation(fields: [user_id], references: [id], onDelete: Cascade)

  @@index([user_id])
}

model tl_kill_logs {
  id               Int      @id @default(autoincrement())
  killer_citizenid String?  @db.VarChar(50)
  killer_name      String?  @db.VarChar(255)
  victim_citizenid String?  @db.VarChar(50)
  victim_name      String?  @db.VarChar(255)
  weapon           String?  @db.VarChar(100)
  created_at       DateTime @default(now()) @db.Timestamp(0)

  @@index([killer_citizenid], map: "killer_citizenid_idx")
  @@index([victim_citizenid], map: "victim_citizenid_idx")
  @@index([created_at], map: "created_at_idx")
}

model tl_gangs {
  id          Int      @id @default(autoincrement())
  name        String   @unique @db.VarChar(50)
  label       String   @db.VarChar(100)
  off_duty_pay Boolean  @default(false) @db.TinyInt
  created_at  DateTime @default(now()) @db.Timestamp(0)
  updated_at  DateTime @default(now()) @db.Timestamp(0)
  grades      tl_gang_grades[]

  @@index([name], map: "idx_name")
  @@index([label], map: "idx_label")
  @@map("tl_gangs")
}

model tl_gang_grades {
  id          Int      @id @default(autoincrement())
  gang_name    String   @db.VarChar(50)
  grade       Int      @db.TinyInt
  name        String   @db.VarChar(100)
  payment     Int      @default(0)
  isboss      Boolean  @default(false) @db.TinyInt
  bankauth    Boolean  @default(false) @db.TinyInt
  created_at  DateTime @default(now()) @db.Timestamp(0)
  updated_at  DateTime @default(now()) @db.Timestamp(0)
  gang        tl_gangs @relation(fields: [gang_name], references: [name], onDelete: Cascade, onUpdate: Cascade)

  @@unique([gang_name, grade], map: "unique_grade")
  @@index([gang_name], map: "idx_gang_name")
  @@index([grade], map: "idx_grade")
  @@index([gang_name, grade], map: "idx_gang_grade_lookup")
  @@map("tl_gang_grades")
}
